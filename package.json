{
  "name": "thlorenz.com",
  "version": "0.1.0-23",
  "description": "Source of thlorenz.com website.",
  "main": "app.js",
  "engines": {
    "node": "~0.8.0",
    "npm": "~1.1.40"
  },
  "subdomain": "thlorenz",
  "domains": [
    "thlorenz.com",
    "www.thlorenz.com"
  ],
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon --ext '.js|.hbs' app.js --dev",
    "css": "cd ./public/css/ && cleancss -o bootstrap.min.css bootstrap.css && cleancss -o bootstrap-responsive.min.css bootstrap-responsive.css && cleancss -o main.min.css main.css cd ../..",
    "deploy": "npm run css && jitsu deploy"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/thlorenz/thlorenz.com.git"
  },
  "keywords": [
    "website",
    "github",
    "blog",
    "stackoverflow"
  ],
  "author": "Thorsten Lorenz <thlorenz@gmx.de> (http://thlorenz.com)",
  "license": "MIT",
  "dependencies": {
    "express": "~3.1.0",
    "browserify": "~1.17.3",
    "npmlog": "0.0.2",
    "hbs": "~2.0.1",
    "browserify-shim": "~0.6.0",
    "ansicolors": "~0.2.1",
    "clone": "~0.1.5",
    "dog": "~0.1.1",
    "moment": "~2.0.0",
    "request": "~2.12.0",
    "uglify-js": "~2.2.5"
  },
  "devDependencies": {
    "nodemon": "~0.7.2",
    "clean-css": "~0.10.1"
  }
}
